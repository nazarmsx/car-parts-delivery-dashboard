{
  "name": "car-parts-delivery-dashboard",
  "version": "1.0.1",
  "description": "Car parts delivery dashboard",
  "scripts": {
    "webpack-dev": "cross-env NODE_ENV=development webpack --mode development",
    "webpack-prod": "cross-env NODE_ENV=production SERVER=prod webpack --mode production",
    "webpack-stage": "cross-env NODE_ENV=production SERVER=stage webpack --mode production",
    "build:dev": "webpack-dev-server --inline --hot --progress --port 8080",
    "build:stage": "npm run build:clean && ngc && npm run webpack-stage && npm run build:clean",
    "build:prod": "npm run build:clean && ngc && npm run webpack-prod && npm run build:clean",
    "build:clean": "rimraf src/**/*.js src/**/*.js.map src/**/*.ngsummary.json src/**/*.metadata.json src/**/**/*.ngfactory.ts src/**/*.ngstyle.ts src/**/*.shim.ts",
    "serve": "lite-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nazarmsx/car-parts-delivery-dashboard"
  },
  "author": "Nazar C. <nazarmsx@ukr.net>",
  "license": "MIT",
  "homepage": "https://github.com/samteb/Angular-7-Webpack-4",
