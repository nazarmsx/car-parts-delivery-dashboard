    "@types/leaflet": "^1.5.5",
    "core-js": "~2.6",
    "hammerjs": "^2.0.8",
    "install": "^0.13.0",
    "leaflet": "^1.6.0",
    "ngx-gallery": "^5.10.0",
    "ngx-toastr": "^10.1.0",
    "randomatic": "^3.1.1",
    "rxjs": "~6.4",
